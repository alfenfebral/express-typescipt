{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-sonarts"
  ],
  "linterOptions": {
    "exclude": [
      "build",
      "node_modules",
      "coverage"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys" : false,
    "no-console" : false,
    "no-duplicate-string": false,
    "no-identical-functions": false
 }
}